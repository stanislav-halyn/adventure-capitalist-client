{
  "name": "adventure-capitalist-client",
  "version": "0.0.0",
  "description": "This is a client for the `adventure-capitalist-game` repository.",
  "main": "./dist/bundle.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stanislav-halyn/adventure-capitalist-client.git"
  },
  "author": "Stanislav Halyn <stasgalin@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stanislav-halyn/adventure-capitalist-client/issues"
  },
  "homepage": "https://github.com/stanislav-halyn/adventure-capitalist-client#readme",
  "scripts": {
    "start": "webpack-dev-server --mode development --config ./webpack/webpack.config.js --open --hot", 
    "build": "webpack --mode production --config ./webpack/webpack.config.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './config/git/commitlint.js'"
    }
  },
  "engines": {
    "node": ">11.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "awesome-typescript-loader": "5.2.1",
    "cz-customizable": "6.2.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "typescript": "3.9.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
}
